{
  "name": "palmabooks-native",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "lint:expo": "expo lint",
    "format:fix": "prettier ./src --write",
    "format:check": "prettier ./src --check",
    "lint": "eslint ./src --max-warnings 0",
    "lint:fix": "eslint ./src --fix",
    "drizzle:generate": "drizzle-kit generate",
    "test:mutate": "stryker run",
    "init": "husky",
    "build:web": "expo export --platform web",
    "build:android": "eas build --platform=android --local",
    "build:check": "tsc --noEmit",
    "fix": "npm run format:fix && npm run lint:fix",
    "pre-commit-check": " npm run build:check && npm run lint && npm run format:check && npm run test",
    "prepare": "husky"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "drizzle-orm": "^0.43.1",
    "expo": "~53.0.0-preview.7",
    "expo-blur": "~14.1.2",
    "expo-constants": "~17.1.2",
    "expo-file-system": "~18.1.3",
    "expo-font": "~13.1.2",
    "expo-haptics": "~14.1.2",
    "expo-linking": "~7.1.2",
    "expo-router": "~5.0.2-preview.4",
    "expo-splash-screen": "~0.30.4",
    "expo-sqlite": "~15.2.4",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.2",
    "expo-system-ui": "~5.0.3",
    "expo-web-browser": "~14.1.3",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@eslint/js": "^9.26.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.26.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.0-preview.3",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "private": true
}
